{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "frontend-task-def",
    "containerDefinitions": [
        {
            "name": "frontend-container",
            "image": "033481624682.dkr.ecr.us-east-1.amazonaws.com/frontend:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "PORT",
                    "value": "80"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/frontend-service",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::033481624682:role/ecsTaskExecutionRole"
}
